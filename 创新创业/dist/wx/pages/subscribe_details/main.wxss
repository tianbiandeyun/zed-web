@import "../../common/vendor.wxss";

page {
  height: 100%;
}
.user_details-container {
  height: 100%;
  border: 40rpx solid #3ba888;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  padding: 20rpx;
  background-color: #fff;
  display: grid;
  align-items: center;
}
.user_details-container > ._div {
  text-align: center;
}
.user_details-container .user_details_tip {
  font-size: 32rpx;
  color: #808695;
}
.user_details-container .user_details_name {
  font-size: 88rpx;
  color: #3ba888;
}
.user_details-container .user_details_time {
  font-size: 32rpx;
  color: #808695;
}
.user_details-container .user_details_time ._span:nth-of-type(1) {
  margin-right: 20rpx;
}
.user_details-container .user_details_start {
  height: 400rpx;
  font-size: 0;
}
.user_details-container .user_details_start ._img {
  width: 100%;
  height: 100%;
}
.user_details-container .user_details-show-box .user_details_show_1 {
  font-size: 40rpx;
  color: #808695;
}
.user_details-container .user_details-show-box .user_details_show_2 {
  height: 20rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  margin-bottom: 16rpx;
}
.user_details-container .user_details-show-box .user_details_show_3 {
  font-size: 28rpx;
  line-height: 1.6;
  color: #808695;
}
.user_details-container .user_details_from {
  height: 120rpx;
  font-size: 0;
}
.user_details-container .user_details_from ._img {
  width: 100%;
  height: 100%;
}